  "cardsdesc" : {
    "3" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "The retreat is weakness, and the weak must be culled.",
      "name" : "Coward!",
      "rule" : "Score this immediately if an enemy fighter begins a Move action adjacent to one of your fighters and ends it adjacent to none of your fighters.",
      "rule34" : "",
      "score" : "1",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "4" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "The Blood God marks well the names of those who commit great acts of slaughter in his name.",
      "name" : "Draw The Gaze Of Khorne",
      "rule" : "Score this immediately if your warband takes two or more enemy fighters out of action in this phase.",
      "rule34" : "",
      "score" : "1",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "7" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "The weak fall, and only the mighty are left standing. Such is the will of Khorne.",
      "name" : "Khorne's Champion",
      "rule" : "Score this in the third end phase if all the fighters except one of your fighters are out of action",
      "rule34" : "",
      "score" : "6",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "8" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "All must wet their blade with the blood of the foe. To fail in this task is to invite the Blood God's wrath.",
      "name" : "Let The Blood Flow",
      "rule" : "Score this immediately if three or more of your fighters made a successful Attack action in this phase.",
      "rule34" : "",
      "score" : "2",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "18" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "The blood is death. The blood is life. The blood must flow.",
      "name" : "Rebirth In Blood",
      "rule" : "Reaction: Play this after an Attack action or ploy that takes your last surviving fighter out of action. Roll a defence dice. If you roll SHIELD or CRITICAL remove all wound tokens from them, and place them on starting hex in your territory.",
      "subtype" : "PLOY",
      "type" : "POWER"
    },
    "23" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "That's a pretty face. I think I'll flay it and hang from it from my belt. - Garrek Gorebeard",
      "name" : "Ever-Advancing",
      "restriction" : "GARREK",
      "rule" : "When this fighter takes an enemy fighter out of action, gain 1 additional glory point.",
      "subtype" : "UPGRADE",
      "type" : "POWER"
    },
    "25" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "That's a pretty face. I think I'll flay it and hang from it from my belt. - Garrek Gorebeard",
      "name" : "Grisly Trophy",
      "restriction" : "GARREK",
      "rule" : "When this fighter takes an enemy fighter out of action, gain 1 additional glory point.",
      "subtype" : "UPGRADE",
      "type" : "POWER"
    },
    "26" : {
      "box" : "core",
      "faction" : "GARREKREAVERS",
      "fluff" : "Karsus' frenzied screams of battle-lust are enough to shake the will of even the sturdiest foe.",
      "name" : "Terrifying Howl",
      "restriction" : "KARSUS",
      "rule" : "Action: Push each adjacent enemy fighter one hex.",
      "subtype" : "UPGRADE",
      "type" : "POWER"
    },
    "31" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "All manner of corrupt and malicious souls haunt the shadowed streets of Shadespire. They shall all fall before the might of Sigmar.",
      "name" : "Cleanse",
      "rule" : "Score this in an end phase if you hold all the objectives in enemy territory.",
      "rule34" : "in any one of your enemies' territories.",
      "score" : "3",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "32" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "This is hallowed land, heathen. Take one step further and feel the wrath of Sigmar.",
      "name" : "Consecrated Area",
      "rule" : "Score this in an end phase if there are no enemy fighters adjacent to your fighters.",
      "rule34" : "",
      "score" : "1",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "34" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "I claim this shard in the name of the God King. Flee, while you still have use of your limbs. - Angharad Brightshield.",
      "name" : "Immovable Object",
      "rule" : "Score this in an end phase if the same friendly fighter has held the same objective at the end of two consecutive action phases.",
      "rule34" : "",
      "score" : "1",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "36" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "Steelheart's band have sworn to end the curse of Shadespire and escape their nightmarish prison. Woe to those who stand in their way.",
      "name" : "Seize Ground",
      "rule" : "Score this in an end phase if you hold an objective in enemy territory.",
      "rule34" : "",
      "score" : "1",
      "subtype" : "",
      "type" : "OBJECTIVE"
    },
    "45" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "Fear not, heathen. I can keep this up all day.",
      "name" : "Tireless Assault",
      "rule" : "Reaction: Play this after a friendly fighter's Attack action that fails. That fighter can make another Attack action that targets the same fighter.",
      "subtype" : "PLOY",
      "type" : "POWER"
    },
    "49" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "'We shall not fall here. Sigmar's light will guide us home.' - Severin Steelheart",
      "name" : "Blessed By Sigmar",
      "rule" : "+1 Wounds",
      "subtype" : "UPGRADE",
      "type" : "POWER"
    },
    "52" : {
      "box" : "core",
      "faction" : "STEELHEARTLIBERATORS",
      "fluff" : "Even amongst his warrior kin, Steelheart's skill with the blade is the stuff of legend.",
      "name" : "Fatal Riposte",
      "restriction" : "STEELHEART",
      "rule" : "Reaction: During an Attack action that targets this fighter and fails, roll an attack dice. On a roll of HAMMER or CRITICAL this fighter cannot be driven back and they can make an Attack action. It must target the attacker.",
      "subtype" : "UPGRADE",
      "type" : "POWER"
    },
    "54" : {
